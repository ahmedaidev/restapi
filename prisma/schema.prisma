generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  price     Int      @default(1)
  title     String   @default("title")
  createdAt DateTime @default(now())
}
